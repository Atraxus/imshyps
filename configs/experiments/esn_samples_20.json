{
    "model": "EchoStateNetwork",
    "metrics": [
        "accuracy"
    ],
    "epochs": 1000,
    "samples": 20,
    "params": {
        "num_units": {
            "type": "int",
            "default": 3000,
            "min": 1000,
            "max": 6000
        },
        "connectivity": {
            "type": "float",
            "default": 0.3,
            "min": 0.1,
            "max": 0.5
        },
        "leakage": {
            "type": "float",
            "default": 0.5,
            "min": 0.1,
            "max": 0.9
        },
        "spectral_radius": {
            "type": "float",
            "default": 0.8,
            "min": 0.1,
            "max": 0.9
        },
        "learning_rate": {
            "type": "float",
            "default": 0.001,
            "min": 0.0001,
            "max": 0.01
        }
    }
}